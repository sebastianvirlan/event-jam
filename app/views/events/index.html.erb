  <body bgcolor="#000000" background="bg_stars.gif" text="#ff0000" link="#ff4c4c" vlink="#ff4c4c" alink="#ff4c4c">
  <center>
    <center>
      <nobr>
        <table border="0" cellpadding="0" cellspacing="0" width="488" height="60">
          <tbody>
            <tr>
              <td align="center"></td>
              <td align="center" width="20"></td>
              <td align="center"></td>
            </tr>
          </tbody>
        </table>
      </nobr>
    </center>
    <br>
    <table width="500" border="0">
    <tbody>
      <tr>
        <td align="left"></td>
        <td align="right"></td>
      </tr>
    </tbody>
  </table>
<br>
<br>
<table width="150%" height="600" border="0">
  <tbody>
    <tr>
      <td colspan="5" align="right" valign="top"></td>
    </tr>
    <tr>
      <% (0..3).each do |n| %>
        <%= render "events/planets/#{@events[n].id}", locals: { event: @events[n] } if @events[n] %>
      <% end %>
    </tr>
    <tr>
      <% (3..6).each do |n| %>
        <%= render "events/planets/#{@events[n].id}" if @events[n] %>
      <% end %>
    </tr>
    <tr>
      <% (7..8).each do |n| %>
        <%= render "events/planets/#{@events[n].id}" if @events[n] %>
      <% end %>
    </tr>
    <tr>
      <td></td>
      <% (9..11).each do |n| %>
        <%= render "events/planets/#{@events[n].id}" if @events[n] %>
      <% end %>
    </tr>
  </tbody>
</table>
<br>
