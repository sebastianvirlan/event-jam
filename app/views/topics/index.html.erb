  <body bgcolor="#000000" background="/bg_stars.gif" text="#ff0000" link="#ff4c4c" vlink="#ff4c4c" alink="#ff4c4c">
    <%= render '/counter'%>
  <% if user_signed_in? %>
    <li>
    <%= link_to('Logout', destroy_user_session_path, method: :delete) %>        
      </li>
    <% else %>
      <li>
      <%= link_to('Login', new_user_session_path)  %>  
    </li>
  <% end %>
  <center>
    <center>
      <nobr>
        <table border="0" cellpadding="0" cellspacing="0" width="488" height="60">
          <tbody>
            <tr>
              <td align="center"></td>
              <td align="center" width="20"></td>
              <td align="center"></td>
            </tr>
          </tbody>
        </table>
      </nobr>
    </center>
    <br>
    <table width="500" border="0">
    <tbody>
      <tr>
        <td align="left"></td>
        <td align="right"></td>
      </tr>
    </tbody>
  </table>
<br>
<br>
<table width="1500" height="600" border="0">
  <tbody>
    <tr>
      <td colspan="5" align="right" valign="top"></td>
    </tr>
    <tr>
      <% (0..3).each do |n| %>
        <%= render "topics/planets/#{@topics[n].id}", locals: { topic: @topics[n] } if @topics[n] %>
      <% end %>
    </tr>
    <tr>
      <%= render "topics/planets/#{@topics[4].id}", locals: { topic: @topics[4] } if @topics[4] %>
      <%= render "topics/planets/6" %>
      <%= render "topics/planets/#{@topics[6].id}", locals: { topic: @topics[6] } if @topics[6] %>
    </tr>
    <tr>
      <% (7..8).each do |n| %>
        <%= render "topics/planets/#{@topics[n].id}", locals: { topic: @topics[n] } if @topics[n] %>
      <% end %>
    </tr>
    <tr>
      <td></td>
      <% (9..11).each do |n| %>
        <%= render "topics/planets/#{@topics[n].id}", locals: { topic: @topics[n] } if @topics[n] %>
      <% end %>
    </tr>
  </tbody>
</table>
<br>
<br>
